<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yangk.demoproject.dao.sys.SysAutoCodeDao">

    <select id="getSysAutoCodeByKey" parameterType="java.lang.String"
            resultType="com.yangk.demoproject.model.sys.SysAutoCode" >
        SELECT
            *
        FROM
            sys_auto_code t1
        WHERE
            t1.auto_code_key = #{autoCodeKey}
    </select>
</mapper>

